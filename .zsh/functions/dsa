#autoload

# dsa - Detect Ssh Agent

ssh_agent_valid && return 0

_dsa_cache=/tmp/.dsa-cache-$USER-$HOST

if [[ -r "$_dsa_cache" ]]; then
  source "$_dsa_cache"
  cat <<EOF 
Read ssh-agent details from cache:
    pid:    $SSH_AGENT_PID
    socket: $SSH_AUTH_SOCK
EOF
  ssh_agent_valid && return 0
fi

recalc_ssh_agent_cache
